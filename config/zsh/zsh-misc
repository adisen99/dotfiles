## Using fortune to echo quotes
fortune ~/.config/quotes/quotes
echo " "

# ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE="fg=white"

## Conda setup
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/aditya/.miniconda/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/aditya/.miniconda/etc/profile.d/conda.sh" ]; then
        . "/home/aditya/.miniconda/etc/profile.d/conda.sh"
    else
        export PATH="/home/aditya/.miniconda/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
